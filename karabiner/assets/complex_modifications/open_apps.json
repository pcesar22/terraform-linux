{
    "title": "Launch apps",
    "rules": [
        {
            "description": "Launch apps by right shift+letters.",
            "manipulators": [
                {
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "mandatory": [
                                "left_command",
                                "left_shift",
                                "left_control",
                                "left_option",
                                "right_shift"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Mail.app'"
                        }
                    ],
                    "type": "basic"
